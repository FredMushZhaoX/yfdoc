<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yf.document.modules.news.mapper.TNewsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yf.document.modules.news.dto.TNewsDTO">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="create_by" property="createBy" />
        <result column="update_by" property="updateBy" />
        <result column="title" property="title" />
        <result column="img_url" property="imgUrl" />
        <result column="news_type" property="newsType" />
        <result column="news_status" property="newsStatus" />
        <result column="text" property="text" />
        <result column="view_count" property="viewCount" />
        <result column="data_flag" property="dataFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        `id`,`create_time`,`update_time`,`create_by`,`update_by`,`title`,`img_url`,`news_type`,`news_status`,`text`,`view_count`,`data_flag`
    </sql>

</mapper>
